{"ast":null,"code":"import { jsx as _jsx } from \"react/jsx-runtime\";\nimport React from 'react';\nimport FacebookLogin from 'react-facebook-login';\nimport Styles from './facebookLogIn.module.scss';\nimport { LoginFacebook } from '../../services/user';\n\nvar FacebookLoginFn = function FacebookLoginFn() {\n  var responseFacebook = function responseFacebook(e) {\n    LoginFacebook({\n      expiresIn: e.expiresIn,\n      accessToken: e.accessToken,\n      id: e.id,\n      urlImage: e.picture.data.url\n    }).then(function (res) {\n      if (res.data.state == false) {\n        alert(res.data.message);\n      } else {\n        fetch(\"api/setToken\", {\n          method: \"post\",\n          headers: {\n            \"Content-Type\": \"application/json\"\n          },\n          body: JSON.stringify({\n            token: res.data.token\n          })\n        }).then(function (data) {\n          localStorage.setItem(\"ref_token\", res.data.ref_token);\n          location.reload();\n          props.openOrcloseModal();\n        });\n      }\n    })[\"catch\"](function (e) {\n      console.log(e);\n    });\n  };\n\n  return /*#__PURE__*/_jsx(\"div\", {\n    className: Styles.Container,\n    children: /*#__PURE__*/_jsx(FacebookLogin, {\n      appId: \"428158131820092\",\n      autoLoad: false,\n      fields: \"name,email,picture\",\n      callback: responseFacebook\n    })\n  });\n};\n\nexport default FacebookLoginFn;","map":null,"metadata":{},"sourceType":"module"}